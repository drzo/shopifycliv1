{
  "name": "@shopify/ui-extensions-dev-console-app",
  "version": "3.46.0-pre.3",
  "private": true,
  "scripts": {
    "build": "nx build",
    "clean": "nx clean",
    "dev": "nx dev",
    "lint": "nx lint",
    "lint:fix": "nx lint:fix",
    "test": "nx run ui-extensions-dev-console:test",
    "test:watch": "nx test:watch"
  },
  "eslintConfig": {
    "plugins": [
      "react",
      "jsx-a11y"
    ],
    "extends": [
      "../../.eslintrc.cjs"
    ],
    "rules": {
      "@typescript-eslint/no-explicit-any": "off",
      "import/extensions": [
        "error",
        "never",
        {
          "ignorePackages": true
        }
      ],
      "jsx-a11y/click-events-have-key-events": "error",
      "react/jsx-no-constructed-context-values": "error",
      "node/no-unsupported-features/node-builtins": [
        "error",
        {
          "version": ">=10.0.0",
          "ignores": []
        }
      ]
    }
  },
  "dependencies": {
    "@preact/signals-core": "^1.2.3",
    "@remote-ui/react": "^5.0.2",
    "@remote-ui/rpc": "^1.4.3",
    "@shopify/app-kit-internal-react": "^0.11.0",
    "@shopify/css-utilities": "^3.1.0",
    "@shopify/dev-console-plugin": "^0.7.0",
    "@shopify/extensibility-host": "^0.7.0",
    "@shopify/extensibility-host-react": "^0.7.0",
    "@shopify/extensibility-host-runtimes": "^0.5.0",
    "@shopify/polaris": "^10.19.0",
    "@shopify/polaris-icons": "^6.15.0",
    "@shopify/react-graphql": "^10.0.2",
    "@shopify/react-i18n": "^6.4.0",
    "@shopify/ui-extensions": "^1.0.2",
    "@shopify/ui-extensions-server-kit": "4.1.0",
    "@shopify/web-worker": "^6.0.5",
    "@vitejs/plugin-react": "^1.3.2",
    "copy-to-clipboard": "^3.3.3",
    "express": "^4.18.2",
    "idb-keyval": "^6.2.0",
    "nodemon": "^2.0.20",
    "qrcode.react": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-reconciler": "^0.29.0",
    "react-router": "^6.6.2",
    "react-router-dom": "^6.6.2",
    "react-toastify": "^9.1.3",
    "react-transition-group": "^4.4.5",
    "uuid": "^9.0.0"
