  "devDependencies": {
    "@shopify/react-testing": "^3.0.0",
    "@shopify/ui-extensions-test-utils": "3.25.0",
    "@types/node": "14.18.36",
    "@types/qrcode.react": "^1.0.2",
    "@types/react": "16.14.0",
    "@types/react-dom": "^16.9.11",
    "@types/uuid": "^9.0.1",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "sass": "^1.60.0",
    "ts-node": "^10.2.1",
    "typescript": "4.9.5",
    "vite": "^4.3.6",
    "vite-tsconfig-paths": "^3.3.14",
    "vitest": "^0.31.0"
  }
}
