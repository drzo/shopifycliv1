{
  "steps": [
    {
      "type": "output",
      "properties": {
        "content": "$ shopify app dev\n"
      }
    },
    {
      "type": "warning",
      "properties": {
        "headline": "Skipping app URL update",
        "body": "You have set `automaticallyUpdateUrls = false` in your shopify.app.production.toml, so the CLI will not update your app's URLs. This might cause the preview feature to not work as expected."
      }
    },
    {
      "type": "output",
      "properties": {
        "content": "âœ” Using your default dev store (shop1) to preview your project.\n"
      }
    },
    {
      "type": "info",
      "properties": {
        "headline": "Here's your app current URL: https://very-cool-app.fly.io",
        "body": [
          "You can update your app's URL anytime in shopify.app.production.toml. But once your app is live, updating its URL will disrupt user access."
        ],
        "nextSteps": []
      }
    },
    {
      "type": "concurrent",
      "properties": {
        "processes": [
          {
            "prefix": "backend",
            "steps": [
              {
                "startMessage": "> shopify-app-template-node@1.0.0 dev",
                "duration": 1,
                "endMessage": "cross-env NODE_ENV=development nodemon index.js --ignore ./frontend"
              }
            ]
          },
          {
            "prefix": "frontend",
            "steps": [
              {
                "startMessage": "shopify-frontend-template-react@1.0.0 dev",
                "duration": 1.5,
                "endMessage": "> Local:    http://localhost:59565/"
              },
              {
                "startMessage": "watching extensions: js,mjs,json",
                "duration": 2,
                "endMessage": "Kick landed!"
              },
              {
                "startMessage": "starting `node index.js",
                "duration": 10,
                "endMessage": "Too late! KO'ed!"
              }
            ]
          }
        ],
        "footer": {
          "shortcuts": [
            {
              "key": "p",
              "action": "View the fight in your browser!"
            },
            {
              "key": "q",
              "action": "Quite fighting! Stop the violence!"
            }
          ]
        }
      }
    }
  ]
}
