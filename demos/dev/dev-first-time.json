{
  "steps": [
    {
      "type": "output",
      "properties": {
        "content": "$ shopify app dev\n"
      }
    },
   {
      "type": "output",
      "properties": {
        "content": "To run this command, log in to Shopify Partners."
      }
    },
    {
      "type": "output",
      "properties": {
        "content": "ðŸ‘‰ Press any key to open the login page on your browser"
      }
    },
    {
      "type": "output",
      "properties": {
        "content": "âœ… Logged in.\n"
      }
    },
    {
      "type": "selectPrompt",
      "properties": {
        "message": "Which Partners organization is this work for?",
        "choices": [
          {
            "label": "Ellen's org",
            "value": "Ellen's org"
          },
          {
            "label": "test partners org",
            "value": "test partners org"
          }
        ]
      }
    },
    {
      "type": "textPrompt",
      "properties": {
        "message": "App name",
        "defaultValue": "Dropshipping (development)"
      }
    },
    {
      "type": "textPrompt",
      "properties": {
        "message": "What would you like to name your configuration file?",
        "defaultValue": "shopify.app.development.toml"
      }
    },
    {
      "type": "info",
      "properties": {
        "headline": "`Dropshipping (development)` connected to this codebase.",
        "body": [
          "shopify.app.development.toml is now the active configuration."
        ],
        "nextSteps": []
      }
    },
    {
      "type": "output",
      "properties": {
        "content": "âœ” Using your default dev store (shop1) to preview your project.\n"
      }
    },
    {
      "type": "info",
      "properties": {
        "headline": "For your convenience, we've given your app a default URL: https://congress-concerned-halifax-spiritual.trycloudflare.com.",
        "body": [
          "You can update your app's URL anytime in shopify.app.development.toml. But once your app is live, updating its URL will disrupt user access."
        ],
        "nextSteps": []
      }
    },
    {
      "type": "concurrent",
      "properties": {
        "processes": [
          {
            "prefix": "backend",
            "steps": [
              {
                "startMessage": "> shopify-app-template-node@1.0.0 dev",
                "duration": 1,
                "endMessage": "cross-env NODE_ENV=development nodemon index.js --ignore ./frontend"
              }
            ]
          },
          {
            "prefix": "frontend",
            "steps": [
              {
                "startMessage": "shopify-frontend-template-react@1.0.0 dev",
                "duration": 1.5,
                "endMessage": "> Local:    http://localhost:59565/"
              },
              {
                "startMessage": "watching extensions: js,mjs,json",
                "duration": 2,
                "endMessage": "Kick landed!"
              },
              {
                "startMessage": "starting `node index.js",
                "duration": 3,
                "endMessage": "Too late! KO'ed!"
              }
            ]
          }
        ],
        "footer": {
          "shortcuts": [
            {
              "key": "p",
              "action": "View the fight in your browser!"
            },
            {
              "key": "q",
              "action": "Quite fighting! Stop the violence!"
            }
          ]
        }
      }
    }
  ]
}
